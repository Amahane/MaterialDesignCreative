<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
	<PropertyGroup>
		<TargetFrameworks>net45;netcoreapp3.0</TargetFrameworks>
		<LangVersion>8.0</LangVersion>
		<Nullable>enable</Nullable>
		<UseWPF>true</UseWPF>
	</PropertyGroup>
	<PropertyGroup>
		<AssemblyTitle>Material Design Creative</AssemblyTitle>
		<Product>Material Design Creative</Product>
		<Description>Material Design Creative is based on Material Design in XAML Toolkit and Material Design Extensions to provide tinny bitty little additional features for WPF Applications.</Description>
		<Version>1.0.1</Version>
		<Authors>Amahane Azure</Authors>
		<Copyright>Copyright Â© Amahane Azure 2020</Copyright>
		<PackageTags>WPF XAML Material Design</PackageTags>
		<PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/Amahane/MaterialDesignCreative</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Amahane/MaterialDesignCreative</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="MaterialDesignThemes" Version="3.0.1" />
	</ItemGroup>
</Project>
